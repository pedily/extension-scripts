{
	"name": "@pedily/extension-scripts",
	"version": "0.0.2",
	"description": "Utility that helps creating Extensions for the Cognigy.AI platform",
	"main": "dist/index.js",
	"keywords": ["cognigy", "cognigy.ai"],
	"scripts": {
		"transpile": "tsc -p .",
		"lint": "eslint src",
		"prettier": "prettier --check src",
		"make-executable": "chmod +x dist/bin/*",
		"build": "npm run lint && npm run prettier && npm run transpile && npm run make-executable"
	},
	"bin": "dist/bin/index.js",
	"author": "Robin Kranich",
	"private": false,
	"license": "MIT",
	"devDependencies": {
		"@types/archiver": "^6.0.2",
		"@types/node": "^20.12.7",
		"@typescript-eslint/eslint-plugin": "^7.7.0",
		"@typescript-eslint/parser": "^7.7.0",
		"eslint": "^8.57.0",
		"prettier": "^3.2.5",
		"typescript": "^5.4.5"
	},
	"dependencies": {
		"archiver": "^7.0.1",
		"commander": "^12.0.0"
	}
}
